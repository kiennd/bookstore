<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="checkout" namespace="/" extends="struts-default">
		<action name="checkout" class = "control.frontend.CheckoutAction" method = "checkout">
			<result name ="success">checkout.jsp</result>
		</action>
		<action name="savecheckout" class = "control.frontend.CheckoutAction" method = "savecheckout">
			<result name ="success">index.jsp</result>
		</action>
    
	</package>
</struts>